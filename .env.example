# .env.example - Tech Playground Configuration Template
# Copie este arquivo para .env e ajuste os valores conforme necessário

# ===== DATABASE =====
DB_HOST=postgres
DB_PORT=5432
DB_NAME=tech_playground
DB_USER=tech_user
DB_PASSWORD=your_secure_password_here
DB_EXTERNAL_PORT=9432

# ===== FASTAPI =====
ENVIRONMENT=development
DEBUG=true
API_TITLE=Tech Playground API
API_VERSION=2.0.0
API_HOST=0.0.0.0
API_PORT=8000
API_EXTERNAL_PORT=9876
LOG_LEVEL=INFO

# ===== CORS =====
ALLOWED_ORIGINS=http://localhost:9876,http://127.0.0.1:9876

# ===== DATA LOADING =====
# IMPORT_CSV: Importa 500 funcionários do arquivo data.csv na primeira execução
# O script detecta dados existentes e não duplica em próximas execuções
IMPORT_CSV=true
